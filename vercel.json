{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "devCommand": "npm run dev:client",
  "env": {
    "VITE_SUPABASE_URL": "",
    "VITE_SUPABASE_ANON_KEY": "",
    "SUPABASE_SERVICE_ROLE_KEY": "",
    "DATABASE_URL": "",
    "NODE_ENV": "production",
    "VITE_API_URL": ""
  },
  "functions": {
    "server/**": {
      "runtime": "nodejs18.x",
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "regions": [
    "iad1"
  ],
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}
